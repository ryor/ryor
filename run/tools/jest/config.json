{
  "collectCoverage": true,
  "collectCoverageFrom": ["**/*.ts", "!**/*.d.ts"],
  "coverageDirectory": "<rootDir>/../coverage",
  "moduleFileExtensions": ["js", "ts"],
  "rootDir": "../../../source",
  "testMatch": ["**/__tests__/*.ts"],
  "transform": {
    "^.+\\.ts$": "<rootDir>/../run/tools/jest/preprocessor.js"
  }
}
