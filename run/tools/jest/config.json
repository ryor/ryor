{
  "collectCoverageFrom": ["source/*.ts", "!source/*.d.ts"],
  "coverageDirectory": "<rootDir>/coverage",
  "moduleFileExtensions": ["js", "ts"],
  "rootDir": "../../..",
  "testEnvironment": "node",
  "testMatch": ["<rootDir>/tests/*.test.js"],
  "transform": {
    "^.+\\.[j|t]s": "<rootDir>/run/tools/jest/preprocessor.js"
  }
}
