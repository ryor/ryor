{
  "collectCoverage": true,
  "collectCoverageFrom": ["source/**/*.ts", "!source/**/*.d.ts"],
  "coverageDirectory": "<rootDir>/coverage",
  "moduleFileExtensions": ["js", "ts"],
  "rootDir": "../../..",
  "testMatch": ["<rootDir>/tests/**/*.ts"],
  "transform": {
    "^.+\\.ts$": "<rootDir>/run/tools/jest/preprocessor.js"
  }
}
